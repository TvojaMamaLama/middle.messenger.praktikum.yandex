{{#> Dialog}}
  <form class="login-page">
    <div>
      {{>PageTitle title="Вход"}}
      {{>InputField className="login-page__input" title="Логин" name="login"}}
      {{>InputField className="login-page__input" title="Пароль" name="password"}}
